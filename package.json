{
  "name": "anti-devtools",
  "version": "1.0.0",
  "description": "Advanced detection and prevention of DevTools usage and userscript injection for web applications.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.mjs",
      "require": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "security",
    "anti-devtools",
    "devtools-detect",
    "userscript-detect"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.3.27",
    "react": "^19.2.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vue": "^3.5.26"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
