{
    "name": "@evelocore/anti-devtools",
    "version": "1.0.1",
    "description": "Advanced detection and prevention of DevTools usage and userscript injection for web applications.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./react": {
            "import": "./dist/react/index.mjs",
            "require": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        },
        "./vue": {
            "import": "./dist/vue/index.mjs",
            "require": "./dist/vue/index.js",
            "types": "./dist/vue/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup && node scripts/obfuscate.js",
        "dev": "tsup --watch",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "security",
        "anti-devtools",
        "devtools-detect",
        "devtools-blocker"
    ],
    "author": "Evelocore",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prabhasha2006/anti-devtools.git"
    },
    "license": "ISC",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/javascript-obfuscator": "^0.0.5",
        "@types/node": "^25.0.3",
        "@types/react": "^18.3.27",
        "javascript-obfuscator": "^5.1.0",
        "react": "^19.2.3",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vue": "^3.5.26"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "vue": ">=3.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "vue": {
            "optional": true
        }
    }
}